{
  "name": "fm-interactive-comments-section",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "description": "This is a solution to the Interactive comments section challenge on Frontend Mentor",
  "author": {
    "name": "Richard Cyrus",
    "email": "hello@rcyrus.com",
    "url": "https://www.richardcyrus.com"
  },
  "repository": {
